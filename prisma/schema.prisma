generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Guestbook {
  id         Int      @id @default(autoincrement())
  message    String
  username   String
  created_at DateTime @default(now())
}
